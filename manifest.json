{
    "manifest_version": 3,
    "name": "IP Extractor by @Gohanckz",
    "version": "1.0",
    "description": "Extrae direcciones IP que están dentro de etiquetas <strong> en una página web",
    "permissions": [
      "activeTab", 
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  